class Solution {
    public int findMin(int[] nums) {
        int left = 0;
        int right = nums.length-1;

        if (nums.length == 1) {
            return nums[0];
        }

        while (right > left) {
            int middle = left + (right-left)/2;
            if (nums[left] < nums[right]) {
                return nums[left];
            } else if (nums[middle] <= nums[right] && nums[middle] <= nums[left]) {
                right = middle;
            } else {
                left = middle+1;
            }
        }
        return Math.min(nums[right],nums[left]);
    }
}
