//Bit Manipulation (more efficient)
class Solution {
    public int singleNumber(int[] nums) {
        int result = 0;
        
        for (int i:nums) {
            result = result ^ i;
        }
        
        return result;
    }
}

//------------OR

class Solution {
    public int singleNumber(int[] nums) {
        List<Integer> list = new ArrayList<>();
        for (int i:nums) {
            if (list.contains(i)) {
                list.remove(Integer.valueOf(i));
            } else {
                list.add(i);
            }
        }
        return list.get(0);
    }
}
