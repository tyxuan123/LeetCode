//Bit Manipulation
public class Solution {
    // you need to treat n as an unsigned value
    // since we need to treat n as an unsigned value, we need to use >>> instead of >>
    public int hammingWeight(int n) {
        int count = 0;
        while (n != 0) {
            count += (n & 1);
            n = n>>>1;
        }
        return count;
    }
}
