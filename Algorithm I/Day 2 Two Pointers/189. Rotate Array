class Solution {
    public void rotate(int[] nums, int k) {
        int index = nums.length - k%nums.length;
        int [] result = new int [nums.length];
        int count = 0;
        
        for (int i=index; i<nums.length; i++) {
            result[count] = nums[i];
            count++;
        }
        
        for (int i=0; i<index; i++) {
            result[count] = nums[i];
            count++;
        }
        
        for (int i=0; i<nums.length; i++) {
            nums[i] = result[i];
        }
    }
}
